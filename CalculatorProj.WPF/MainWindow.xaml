<Window x:Class="CalculatorProj.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculatorProj.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="tFa" AutomationProperties.AutomationId="fa"/>
            <TextBox x:Name="tSa" AutomationProperties.AutomationId="sa"/>
            <Button Content="Summ" Click="Button_Click" AutomationProperties.AutomationId="addBtn"/>
            <TextBlock x:Name="tbRes" AutomationProperties.AutomationId="res"/>
        </StackPanel>
    </Grid>
</Window>
